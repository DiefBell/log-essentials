{
  "name": "log-essentials",
  "version": "2.0.0",
  "description": "Basic logger with support for namespaces, color, levels and timestamps.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm ./lib/* && tsc --project tsconfig.build.json",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "no-circ-deps": "dpdm --no-warning --no-tree --exit-code circular:1 ./src/index.ts",
    "test": "node example.js"
  },
  "files": [
    "lib/",
    "README.md",
    "LICENSE.txt"
  ],
  "keywords": [
    "log",
    "fancy",
    "essential",
    "logger",
    "colors",
    "levels",
    "timestamp",
    "icons",
    "console",
    "error",
    "warning",
    "namespace",
    "prefix"
  ],
  "author": "Tim Strasser",
  "license": "ISC",
  "dependencies": {
    "colors": "^1.4.0",
    "fancy-log": "^2.0.0",
    "log-symbols": "^5.1.0"
  },
  "devDependencies": {
    "@types/fancy-log": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "dpdm": "^3.9.0",
    "eslint": "^8.15.0",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XENOTpro/log-essentials.git"
  },
  "bugs": {
    "url": "https://github.com/XENOTpro/log-essentials/issues"
  },
  "homepage": "https://github.com/XENOTpro/log-essentials#readme"
}
